    "moment": "^2.24.0",
    "moment-weekday-calc": "^1.1.4",
    "prop-types": "^15.7.2",
    "rc-slider": "^9.2.1",
    "react": "^16.12.0",
    "react-datepicker": "^2.13.0",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.12.0",
    "react-icons": "^3.9.0",
    "react-number-format": "^4.4.1",
    "react-rebound": "^0.8.3",
